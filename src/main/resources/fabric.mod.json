{
	"schemaVersion": 1,
	"id": "nexusutils",
	"version": "${version}",
	"name": "Nexus Utils",
	"description": "Nexus mod for better life",
	"authors": [
		"Im_Banana"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "MIT",
	"icon": "assets/nexusutils/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"me.imbanana.nexusutils.NexusUtils"
		],
		"client": [
			"me.imbanana.nexusutils.NexusUtilsClient"
		],
		"fabric-datagen": [
			"me.imbanana.nexusutils.NexusUtilsDataGenerator"
		]
	},
	"mixins": [
		"nexusutils.mixins.json"
	],
	"custom": {
		"loom:injected_interfaces": {
			"net/minecraft/entity/projectile/TridentEntity": ["me/imbanana/nexusutils/util/TridentEntityItemStackGetter"]
		}
	},
	"depends": {
		"fabricloader": ">=0.14.23",
		"minecraft": "~1.20.2",
		"java": ">=17",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}